{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "braintree/braintree_php": "^6.13",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16",
        "guzzlehttp/guzzle": "^7.8",
        "guzzlehttp/psr7": "^2.6",
        "nelmio/cors-bundle": "^2.3",
        "shopware/app-bundle": "^2.0",
        "symfony/console": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^7.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/property-access": "^7.0",
        "symfony/psr-http-message-bridge": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/uid": "^7.0",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "^7.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "infection/extension-installer": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DoctrineMigrations\\": "migrations/",
            "Swag\\Braintree\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swag\\Braintree\\Tests\\Unit\\": "tests/unit/",
            "Swag\\Braintree\\Tests\\Integration\\": "tests/integration/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "auto-scripts": {
        "cache:clear": "symfony-cmd",
        "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
        "@auto-scripts"
    ],
    "post-update-cmd": [
        "@auto-scripts"
    ],
    "scripts": {
        "ecs": "ecs check src tests",
        "ecs-fix": "ecs check --fix src tests",
        "phpstan": "phpstan analyze --memory-limit=2G",
        "phpunit": "phpunit",
        "infection": "infection -j10 -s",
        "eslint": "npm run eslint",
        "eslint-fix": "npm run eslint-fix",
        "setup": [
            "bin/console doctrine:database:drop --force",
            "bin/console doctrine:database:create",
            "bin/console doctrine:migrations:migrate --force",
            "@setup:url"
        ],
        "setup:url": [
            "bin/console setup:url",
            "npm run dev"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "require-dev": {
        "infection/infection": "^0.27.0",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.16",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "^10",
        "symfony/debug-bundle": "7.0.*",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "7.0.*",
        "symplify/easy-coding-standard": "^12.0",
        "symplify/phpstan-rules": "12.4.9",
        "tomasvotruba/type-coverage": "^0.2.1"
    }
}
